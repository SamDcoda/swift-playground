//multiple steps 7 x2
//repeate 3 times

func moveToggleGoBack() {
    for i in 1 ... 7 {
        moveForward()
    }
    turnLeft()
    turnLeft()
    toggleSwitch()
    for i in 1 ... 7 {
        moveForward()
    }
    turnRight()
}
func solveStairs() {
    moveForward()
    moveForward()
    turnRight()
    moveToggleGoBack()
}

for i in 1 ... 3 {
    solveStairs()
}
